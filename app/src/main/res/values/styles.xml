<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="SpUsage">

    <style name="LexendTextViewStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/lexend_daca_regular_400</item>
    </style>
    <style name="BottomNavigationView.Active" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">@dimen/_14sdp</item>
    </style>

    <style name="BottomNavigationView" parent="@style/TextAppearance.AppCompat.Caption">
        <item name="android:textSize">@dimen/_14sdp</item>
